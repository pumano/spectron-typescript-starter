{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [false, "log", "error"],
    "interface-name": [true, "never-prefix"]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}